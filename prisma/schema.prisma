generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DB_URI")
}

model sections {
  id    String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  title String
}

model tecnologies {
  id   String @id @default(dbgenerated()) @map("_id") @db.ObjectId
  logo String
  name String
  url  String
}
